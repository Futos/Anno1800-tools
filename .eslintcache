ductionChain.js",["86"],"/mnt/storage/vscode/Anno1800-tools/src/helpers/helpers.js",["87"],"export const findCharWithTollerance = (src, txt) => {\n  if (!src || !txt) {\n    return ''\n  }\n  const re = new RegExp(txt.split('').map(function (a, b, c) { return txt.substr(0, b) + a + '?.?' + txt.substr(b + 1) }).join('|'), 'gi')\n  return src.match(re) != null\n}\n\nexport const umlautify = string => {\n  let value = string\n  value = value.replace(/ä/g, 'ae')\n  value = value.replace(/ö/g, 'oe')\n  value = value.replace(/ü/g, 'ue')\n  value = value.replace(/ß/g, 'ss')\n  value = value.replace(/Ä/g, 'Ae')\n  value = value.replace(/Ö/g, 'Oe')\n  value = value.replace(/Ü/g, 'Ue')\n  return value\n}\n\nexport const searchify = val => {\n  let returnVal = val\n  returnVal = returnVal.toLowerCase()\n  returnVal = umlautify(returnVal)\n  returnVal = encodeURI(returnVal)\n\n  return returnVal\n}\n\nexport default {\n  findCharWithTollerance,\n  umlautify,\n  searchify\n}\n","/mnt/storage/vscode/Anno1800-tools/src/pages/Gebaeude/Gebaeude.js",[],"/mnt/storage/vscode/Anno1800-tools/src/helpers/prodDataHelpers.js",["88"],"/mnt/storage/vscode/Anno1800-tools/src/helpers/calculations.js",["89"],"export const ratioCalculator = (input1, input2) => {\n  return input1 / input2\n}\n\nexport const ratioCalculatorReverse = (input1, input2) => {\n  return input2 / input1\n}\n\nexport const roundUp = function (val) {\n  return Math.ceil(val)\n}\n\nexport default {\n  ratioCalculator,\n  ratioCalculatorReverse,\n  roundUp\n}\n","/mnt/storage/vscode/Anno1800-tools/src/components/ProductionChains/ProductionCard.js",["90"],"/mnt/storage/vscode/Anno1800-tools/src/assets/dummy/productionTypes.js",["91"],{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":7,"column":10,"nodeType":"98","messageId":"99","endLine":7,"endColumn":18},{"ruleId":"96","severity":1,"message":"100","line":1,"column":8,"nodeType":"98","messageId":"99","endLine